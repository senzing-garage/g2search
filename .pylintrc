[pylint]
disable=
    bare-except,
    broad-except,
    consider-using-dict-items,
    consider-using-f-string,
    consider-using-with,
    eval-used,
    f-string-without-interpolation,
    import-error,
    inconsistent-return-statements,
    invalid-name,
    line-too-long,
    logging-fstring-interpolation,
    logging-not-lazy,
    missing-function-docstring,
    missing-module-docstring,
    multiple-statements,
    no-else-break,
    no-else-continue,
    redefined-outer-name,
    superfluous-parens,
    too-many-arguments,
    too-many-branches,
    too-many-lines,
    too-many-locals,
    too-many-statements,
    trailing-whitespace,
    undefined-variable,
    ungrouped-imports,
    unidiomatic-typecheck,
    unspecified-encoding,
    unused-argument,
    unused-import,
    unused-variable,
    useless-return,
    using-constant-test,
good-names=
    template-python
ignore=
    __init__.py,
notes=
    FIXME,
